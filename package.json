{
  "name": "racktools",
  "version": "1.0.0",
  "description": "Outils techniques audiovisuels",
  "author": "Thomas Gouaz√©",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "dev-vite": "vite",
    "build": "vite build",
    "start": "electron .",
    "start-electron": "electron .",
    "osc-server": "node osc-server.js",
    "dist": "electron-builder"
  },
  "dependencies": {
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.513.0",
    "osc": "^2.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flow-renderer": "^10.3.17",
    "react-router-dom": "^7.6.1",
    "sacn": "^4.6.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "vite": "^6.3.5",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.keutu.racktools",
    "productName": "RackTools",
    "icon": "assets/icon.ico",
    "asarUnpack": [
      "server/osc-server.js"
    ],
    "files": [
      "dist/",
      "main.js",
      "preload.js",
      {
        "from": "server",
        "to": "server",
        "filter": [
          "osc-server.js"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets"
      }
    ],
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
